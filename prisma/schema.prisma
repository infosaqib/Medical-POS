generator client {
provider = "prisma-client"
output = "../generated/prisma"
}

datasource db {
provider = "postgresql"
}

model medicine {
id Int @id @default(autoincrement())
title String
stock String
expiry Int
desc String?
}

model customer {
id Int @id @default(autoincrement())
name String
contact Int
address String
country String?
}
